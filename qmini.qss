/*--------QLabel-------*/
QLabel, QWidget, QElidedLabel
{
    color: #000;
    background-color: #dcdccc;
    /* background-color: #262729; */
}
/*-----QToolBar-----*/
QToolBar
{
	/* background-color: #262729; */
    background-color: #dcdccc;
	color: #000;
	border-color: #000000;

}
/*QMenu*/
QMenu {
    margin: 10px;  /*A bit of nice padding around menu items.*/
    background-color: #dcdccc;
    color: #000;
    border: 1px solid #DCDCCC;
}

QMenu::item,
QMenuBar::item
{
    background-color: #dcdccc;
    color: #000;
}

QMenu::item:disabled,
QMenuBar::item:disabled
{
    color: #989899;
    font-style: italic;
}

QMenu::item:selected,
QMenuBar::item:selected
{
    color: #fff;
    background-color: #233323;
    border-color: #4de661;
}
