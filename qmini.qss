
/*QTreeView*/
QTreeView
{
    background-color: #dcdccc;
    
}

QHeaderView::section:horizontal{
    background-color: #dcdccc;
    border-style: none;
    color: black; 
    border: 1px solid #3f3f3f;
    padding: 5px;
    }
/*-----QToolButton-----*/
QToolButton
{
	background-color: #dcdccc;
    /* #00ff4f;*/
	color: #000000;
	font-weight: bold;
	border-style: solid;
	border-color: #4de661;
	padding: 3px;

}
QToolButton::menu-indicator { width: 0px; }

QToolButton::hover
{
	background-color: #000;
    color: #fff;
    border-color: #4de661;
    /* border: 1px; */
    border: #4de661;
}


QToolButton::pressed
{
	background-color: #233323;
    color: #fff;
    border-color: #177226;
}
